{
  "name": "prind-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.35.0",
    "@blueprintjs/datetime": "^3.19.3",
    "@blueprintjs/select": "^3.14.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "aws-amplify": "^3.3.7",
    "aws-cognito-promises": "^1.6.4",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.3",
    "env-cmd": "^10.0.1",
    "hash.js": "^1.1.7",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-ga": "^2.7.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-saga": "^1.1.3",
    "stringquery": "^1.0.8",
    "typescript": "^3.9.7",
    "validator": "^12.2.0"
  },
  "scripts": {
    "s3Sync:dev": "aws s3 sync build/ s3://prind-portal-dev --profile prind",
    "s3Sync:staging": "aws s3 sync build/ s3://prind-portal-staging --profile prind",
    "s3Sync:production": "aws s3 sync build/ s3://prind-portal --profile prind",
    "cfInvalidate:dev": "aws cloudfront create-invalidation --distribution-id E1D2QKI54UR0JR --paths /\\*  --profile prind",
    "cfInvalidate:staging": "aws cloudfront create-invalidation --distribution-id E3GZUKI7SBH38L --paths /\\*  --profile prind",
    "cfInvalidate:production": "aws cloudfront create-invalidation --distribution-id E1J4MRU1DA3F0B --paths /\\*  --profile prind",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "deploy:dev": "npm run build:dev && npm run s3Sync:dev && npm run cfInvalidate:dev",
    "deploy:staging": "npm run build:staging && npm run s3Sync:staging && npm run cfInvalidate:staging",
    "deploy:production": "npm run build:production && npm run s3Sync:production && npm run cfInvalidate:production",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "testEcho": "echo $PATH"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.7.0"
  }
}

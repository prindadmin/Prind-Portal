{
  "name": "prind-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/datetime": "^3.15.2",
    "@blueprintjs/select": "^3.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "aws-cognito-promises": "^1.6.4",
    "aws-sdk": "^2.610.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "hash.js": "^1.1.7",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-saga": "^1.1.3",
    "stringquery": "^1.0.8",
    "typescript": "^3.7.5",
    "validator": "^12.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "s3SyncDev": "aws s3 sync build/ s3://prind-portal-dev --profile prind",
    "cfInvalidateDev": "aws cloudfront create-invalidation --distribution-id E1D2QKI54UR0JR --paths /\\*  --profile prind",
    "s3SyncProduction": "aws s3 sync build/ s3://prind-portal --profile prind",
    "cfInvalidateProduction": "aws cloudfront create-invalidation --distribution-id E1J4MRU1DA3F0B --paths /\\*  --profile prind",
    "test": "react-scripts test",
    "devDeploy": "npm run build && npm run s3SyncDev && npm run cfInvalidateDev",
    "productionDeploy": "npm run build && npm run s3SyncProduction && npm run cfInvalidateProduction",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "testEcho": "echo $PATH"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.5.0"
  }
}
